      object daSQL2: TdaSQL
        GuidCollationType = gcMSAccess
        DatabaseName = 'ADOConnection'
        DatabaseType = dtMSAccess
        DataPipelineName = 'DS_Schuelerzahlen'
        EditSQLAsText = True
        IsCaseSensitive = True
        LinkColor = clNavy
        LinkType = ltParameterizedSQL
        MaxSQLFieldAliasLength = 25
        SQLText.Strings = (
          'SELECT EigeneSchule_Faecher.FachKrz, '
          '       Kurse.KurzBez, Kurse.LehrerKrz, '
          '       Schueler.Name, Schueler.Vorname, '
          '       Schueler.ID'
          'FROM ((((Schueler Schueler'
          
            '      INNER JOIN SchuelerLernabschnittsdaten SchuelerLernabschni' +
            'ttsdaten ON '
          '     (SchuelerLernabschnittsdaten.Schueler_ID = Schueler.ID)'
          
            '      AND (SchuelerLernabschnittsdaten.Jahr = Schueler.AktSchulj' +
            'ahr)'
          
            '      AND (SchuelerLernabschnittsdaten.Abschnitt = Schueler.AktA' +
            'bschnitt) )'
          
            '      INNER JOIN SchuelerLeistungsdaten SchuelerLeistungsdaten O' +
            'N '
          
            '     (SchuelerLeistungsdaten.Abschnitt_ID = SchuelerLernabschnit' +
            'tsdaten.ID) )'
          '      INNER JOIN EigeneSchule_Faecher EigeneSchule_Faecher ON '
          
            '     (EigeneSchule_Faecher.ID = SchuelerLeistungsdaten.Fach_ID) ' +
            ')'
          '      INNER JOIN Kurse Kurse ON '
          '     (Kurse.ID = SchuelerLeistungsdaten.Kurs_ID) )'
          'WHERE ( Schueler.Status IN (2,6) )'
          
            '       AND ( SchuelerLeistungsdaten.Kursart IN ('#39'LK1'#39','#39'LK2'#39','#39'AB3' +
            #39'))'
          '       AND ( Schueler.ASDJahrgang IN ('#39'Q2'#39','#39'13'#39') )'
          '       AND ( Schueler.Geloescht <> '#39'+'#39' )'
          'GROUP BY EigeneSchule_Faecher.FachKrz, '
          '         Kurse.KurzBez, Kurse.LehrerKrz, '
          '         Schueler.Name, Schueler.Vorname, '
          '         Schueler.ID'
          'ORDER BY EigeneSchule_Faecher.FachKrz, '
          '         Kurse.KurzBez')
        SQLType = sqSQL2
